  {/* To make it safer to implement use like this: */}\r\n            {/* <button onClick={() => setCount(prevCount => prevCount + 1)}>Increment</button>\r\n            <button onClick={() => setCount(prevCount => prevCount - 1)}>Decrement</button> */}\r\n\r\n            {/* Hooks with objects */}\r\n            <form>\r\n                <h6>Hooks with objects</h6>\r\n                <input type='text' value={name.firstName} onChange={ e => setName({ ...name, firstName: e.target.value})} placeholder='write your first name'/>\r\n                <input type='text' value={name.lastName} onChange={ e => setName({ ...name, lastName: e.target.value})} placeholder='write your last name'/>\r\n                <h4>Your first name is {name.firstName}</h4>\r\n                <h4>Your last name is { name.lastName}</h4>\r\n            </form>\r\n\r\n            <h5>Array hooks</h5> \r\n            <button onClick={addItem}>Add an item to array</button>\r\n            <ul>\r\n                {\r\n                    items.map(item => (\r\n                        <li key={item.id}>{ item.value}</li>\r\n                    ))\r\n                }\r\n            </ul>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default HookCounter\r\n\r\n","C:\\Users\\elchi\\Desktop\\React Tutorials\\hello-world\\src\\components\\UseEffectDemo.js",[],"C:\\Users\\elchi\\Desktop\\React Tutorials\\hello-world\\src\\components\\HookMouse.js",[],{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","severity":1,"message":"165","line":17,"column":8,"nodeType":"166","messageId":"167","endLine":17,"endColumn":14},{"ruleId":"164","severity":1,"message":"168","line":1,"column":17,"nodeType":"166","messageId":"167","endLine":1,"endColumn":26},"no-native-reassign",["169"],"no-negated-in-lhs",["170"],"no-unused-vars","'styles' is defined but never used.","Identifier","unusedVar","'Component' is defined but never used.","no-global-assign","no-unsafe-negation"]